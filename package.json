{
  "name": "@sky-ph/atlas",
  "version": "0.2.5",
  "license": "AGPL-3.0-only",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./editors": {
      "types": "./dist/editors.d.ts",
      "import": "./dist/es/editors.js",
      "require": "./dist/cjs/editors.js"
    },
    "./document-models": {
      "types": "./dist/documentModels.d.ts",
      "import": "./dist/es/documentModels.js",
      "require": "./dist/cjs/documentModels.js"
    },
    "./subgraphs": {
      "types": "./dist/subgraphs.d.ts",
      "import": "./dist/es/subgraphs.js",
      "require": "./dist/cjs/subgraphs.js"
    },
    "./manifest": {
      "types": "./dist/manifest.d.ts",
      "require": "./dist/cjs/manifest.js",
      "import": "./dist/es/manifest.js"
    }
  },
  "scripts": {
    "generate": "ph-cli generate",
    "dev": "ph-cli dev --config-file ./powerhouse.config.json",
    "format-generated": "npx prettier \"**/{gen,processors,editors}/**/*\" --write --ignore-path --no-error-on-unmatched-pattern >> /dev/null",
    "postgenerate": "npm run format-generated",
    "check-types": "tsc",
    "postlint": "npm run check-types",
    "lint": "eslint",
    "build": "npm run generate && vite build",
    "storybook": "npm run install:storybook && npm run generate && storybook dev -p 6006",
    "build-storybook": "npm run generate && storybook build",
    "test": "vitest run",
    "test:nogen": "vitest run",
    "test:watch": "vitest",
    "connect": "ph-cli connect",
    "reactor": "ph-cli reactor",
    "install:storybook": "tsx scripts/index.ts",
    "service": "ph-cli service",
    "service-startup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-startup.sh",
    "service-unstartup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-unstartup.sh",
    "script:apply-changes": "bun run ./scripts/apply-changes.ts",
    "script:fetch-data": "bun run ./scripts/fetch-data.ts",
    "script:generate-gql-clients": "bun run ./scripts/gen-clients.ts",
    "script:generate-all": "bash ./docmodel-zips/generate-all.sh"
  },
  "peerDependencies": {
    "@powerhousedao/reactor-browser": "^1.11.0",
    "document-model": "^2.21.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@powerhousedao/analytics-engine-core": "^0.3.2",
    "@powerhousedao/builder-tools": "^0.2.7",
    "@powerhousedao/codegen": "dev",
    "@powerhousedao/config": "dev",
    "@powerhousedao/design-system": "1.22.5-canary.46",
    "@powerhousedao/mips-parser": "2.2.2-alpha.0",
    "@powerhousedao/ph-cli": "dev",
    "@powerhousedao/reactor-api": "dev",
    "@powerhousedao/reactor-browser": "dev",
    "@powerhousedao/reactor-local": "dev",
    "@powerhousedao/scalars": "dev",
    "@storybook/addon-essentials": "^8.6.3",
    "@storybook/addon-interactions": "^8.6.3",
    "@storybook/addon-links": "^8.6.3",
    "@storybook/blocks": "^8.6.3",
    "@storybook/components": "^8.6.3",
    "@storybook/manager-api": "^8.6.3",
    "@storybook/preview-api": "^8.6.3",
    "@storybook/react": "^8.6.3",
    "@storybook/react-vite": "^8.6.3",
    "@storybook/test": "^8.6.3",
    "@storybook/types": "^8.6.3",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.13.9",
    "@types/react": "^18.3.18",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "autoprefixer": "^10.4.20",
    "document-drive": "dev",
    "document-model": "dev",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.15.0",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "package-manager-detector": "^0.2.11",
    "pm2": "^5.4.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.6.3",
    "storybook-mock-date-decorator": "^2.0.6",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-generate-file": "^0.2.0",
    "vite-plugin-graphql-loader": "^4.0.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.9",
    "zod": "^3.24.2"
  },
  "description": "",
  "dependencies": {
    "@powerhousedao/common": "dev",
    "@types/diff": "^7.0.1",
    "@types/lodash": "^4.17.16",
    "diff": "^7.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-ts-client": "^10.4.0",
    "lodash": "^4.17.21"
  }
}
